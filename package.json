{
  "name": "tls-certificate-builder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:wasm && vite build",
    "build:wasm": "npm run build:wasm:nodejs && npm run build:wasm:web",
    "build:wasm:nodejs": "cd cert-parser-wasm && cargo install wasm-pack --quiet 2>/dev/null || true && wasm-pack build --target nodejs --out-dir pkg",
    "build:wasm:web": "cd cert-parser-wasm && cargo install wasm-pack --quiet 2>/dev/null || true && wasm-pack build --target web --out-dir pkg-web",
    "prebuild": "npm run build:wasm",
    "pretest": "npm run build:wasm:nodejs",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "dependencies": {
    "@xyflow/react": "^12.9.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "vite": "^7.2.4"
  }
}
